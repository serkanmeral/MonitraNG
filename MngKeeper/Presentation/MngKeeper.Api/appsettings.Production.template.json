{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning",
        "MngKeeper": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Warning"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "${SEQ_URL}",
          "apiKey": "${SEQ_API_KEY}",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ]
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5001"
      },
      "Https": {
        "Url": "https://localhost:5443",
        "Certificate": {
          "Path": "${CERT_PATH}",
          "Password": "${CERT_PASSWORD}"
        }
      }
    }
  },
  "ConnectionStrings": {
    "MongoDB": "${MONGODB_CONNECTION}",
    "RabbitMQ": "${RABBITMQ_CONNECTION}",
    "Redis": "${REDIS_CONNECTION}"
  },
  "Keycloak": {
    "BaseUrl": "${KEYCLOAK_URL}",
    "AdminUsername": "${KEYCLOAK_ADMIN_USER}",
    "AdminPassword": "${KEYCLOAK_ADMIN_PASSWORD}"
  },
  "MongoDB": {
    "DatabaseName": "mngkeeper"
  },
  "Mqtt": {
    "BrokerHost": "${MQTT_HOST}",
    "BrokerPort": 1883,
    "Username": "${MQTT_USER}",
    "Password": "${MQTT_PASSWORD}",
    "TopicPrefix": "MNG"
  },
  "RabbitMQ": {
    "Host": "${RABBITMQ_HOST}",
    "Port": 5672,
    "Username": "${RABBITMQ_USER}",
    "Password": "${RABBITMQ_PASSWORD}",
    "VirtualHost": "/"
  }
}

